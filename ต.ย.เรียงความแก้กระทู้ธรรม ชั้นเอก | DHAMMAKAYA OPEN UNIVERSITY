.canvasWrapper svg[data-main-rotation="180"] mask, .canvasWrapper svg[data-main-rotation="180"] use:not(.clip, .mask) { transform: matrix(-1, 0, 0, -1, 1, 1); }

.canvasWrapper svg[data-main-rotation="270"] mask, .canvasWrapper svg[data-main-rotation="270"] use:not(.clip, .mask) { transform: matrix(0, -1, 1, 0, 0, 1); }

